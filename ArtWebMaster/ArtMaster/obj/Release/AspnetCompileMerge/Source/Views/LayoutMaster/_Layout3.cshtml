<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="robots" content="noindex,nofollow" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @Scripts.Render("~/bundles/scripts")
    @Styles.Render("~/bundles/artstyle")
    @Styles.Render("~/bundles/font")
    @Styles.Render("~/bundles/goofonts")

    <script type="text/javascript" src="https://ecomfe.github.io/echarts-examples/public/vendors/echarts/echarts-all-3.js"></script>

    <style>
        #localize {
            margin-right: 15px;
        }
    </style>
    <script>
        $(document).ready(function () {
            $(function () {
                //   $(this).bind("contextmenu", function (e) {
                //    e.preventDefault();
                //   });
            });
            $(document).keydown(function (event) {
                if (event.keyCode == 123) {
                    return false;
                }
                else if (event.ctrlKey && event.shiftKey && event.keyCode == 73) {
                    return false;  //Prevent from ctrl+shift+i
                }

                var pressedKey = String.fromCharCode(event.keyCode).toLowerCase();
                if (event.ctrlKey && pressedKey == "u") {
                    return false;
                }
            });

            $('#localize').on('change', function () {
                document.forms["lay-resource-form"].submit();
            });

          
          
        });
    </script>
</head>

<body class="bg-white"  onunload="">
    <!-- wrapper section starts-->
    <div class="wrapper">
        <!-- header section  start-->
        <header class="header-blue">
            <div class="main-container">
                <a href="#" class="logo" title="ART"></a>

                <form action="/Home/LogOut" method="post" id="lay-logout">
                    <ul class="top-menu">
                        <li><a href="../Reports/Dashboard" id="aDashboard" title="Dashboard">Dashboard</a></li>
                        <li><a href="../Reports/GSDView" id="aGsdview" title="GSD View">gsd view</a></li>
                        <li><a href="../Reports/EmployeeSearch" id="aSearch" title="Employee Search">search</a></li>
                        <li><a href="javascript:void(0);" class="logout" title="Logout"><img src="../Images/ArtImages/log-out.png"></a></li>
                    </ul>
                </form>
            </div>
        </header>
        <!-- header section  end-->
        <!-- container section  start-->
        <div class="content-container">
            @RenderBody()
        </div>
        <!-- container section  end-->
    </div>
    <!-- wrapper section ends-->
    <!-- footer section starts-->
    <footer>
        <p>@Art.ArtCopyRights</p>
    </footer>
    <!-- footer section ends-->
    <script>
        $(document).ready(function () {
            $('.logout').on('click', function () {
                fnLogOut();
            })
            $('.logout-wo-border').on('click', function () {
                fnLogOut();
            })
        });
        function fnLogOut() {
            $("#lay-logout").submit();
        }
        function noBack() {
            window.history.forward();
        }
    </script>

</body>

</html>
