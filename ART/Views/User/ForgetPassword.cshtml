        </div>
        <div>
            <select id="Que2" name="Que2"></select>
            <input type="text" name="txtQue2Ans" id="txtQue1Ans">
        </div>
        <div>
            <select id="Que3" name="Que3"></select>
            <input type="text" name="txtQue3Ans" id="txtQue1Ans">
        </div>
    </div>

    <div>
        <button type="submit">Submit</button>
    </div>
</form>
<script src="~/Scripts/ART%20Script/ForgetPassword.js"></script>

<script type="text/javascript">
    $(document).ready(function () {
        forgetPassword.init();
    });
</script>
