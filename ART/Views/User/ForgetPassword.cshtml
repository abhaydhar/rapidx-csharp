@{
    ViewBag.Title = "ForgetPassword";
    Layout = "~/Views/LayoutMaster/_DefaultLayout.cshtml";
}

<h2>ForgetPassword</h2>

<form class="form-horizontal" role="form" id="forgetpass-form" method="post" action="/User/ForgetPassword" autocomplete="off">
